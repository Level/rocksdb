{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${HOME}/.nvm/versions/node/v18*/include/node/**",
                "${workspaceFolder}/node_modules/napi-macros/**",
                "${workspaceFolder}/deps/aws-sdk-cpp/include/**",
                "${workspaceFolder}/deps/rocksdb-cloud/rocksdb-cloud/include/**"
            ],
            "defines": [
                "USE_AWS=1",
                "ZSTD=1"
            ],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-arm64"
        }
    ],
    "version": 4
}